<% if @project.persisted? -%>
  const newItem = $('<%= j render partial: 'projects/project_menu_item', object: @project %>');
  $('#project-menu').prepend(newItem);
  const menuButton = document.querySelector('#project-menu-button');
  const menu = menuButton.MaterialExtMenuButton.getMenuElement();
  menuButton.MaterialExtMenuButton.setSelectedMenuItem(menu.children[0]);
  $('.empty-state').remove()
  $.ajax({url: '<%= edit_project_path(@project) %>'});
<% end -%>
